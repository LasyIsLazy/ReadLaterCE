{
    "manifest_version": 2,
    "name": "Read Later",
    "short_name": "RL",
    "description": "Save Urls to read later and sync between browsers.",
    "permissions": [
        "storage",
        "tabs"
    ],
    "version": "0.0.0.5",
    "browser_action": {
        "default_title": "Read Later",
        "default_icon": "assets/img/icon.png",
        "default_popup": "popup.html"
    }
}
